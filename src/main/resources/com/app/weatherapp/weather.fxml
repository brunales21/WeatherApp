<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="600.0" maxWidth="400.0" prefHeight="600.0" prefWidth="400.0" styleClass="weather" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.weatherapp.WeatherController">
   <children>
      <Pane fx:id="pane" layoutX="110.0" layoutY="35.0" prefHeight="231.0" prefWidth="192.0">
         <VBox prefHeight="231.0" prefWidth="190.0">
            <children>
               <ImageView fx:id="weatherIcon" fitHeight="115.0" fitWidth="155.0" pickOnBounds="true" preserveRatio="true">
                  <VBox.margin>
                     <Insets left="35.0" />
                  </VBox.margin></ImageView>
               <Label fx:id="degrees" alignment="CENTER" prefHeight="73.0" prefWidth="193.0" text="Grados">
                  <font>
                     <Font size="26.0" />
                  </font></Label>
               <Label fx:id="city" alignment="CENTER" prefHeight="43.0" prefWidth="196.0" text="Ciudad">
                  <font>
                     <Font size="21.0" />
                  </font></Label>
            </children>
         </VBox>
      </Pane>
      <HBox fx:id="hbox" alignment="TOP_CENTER" layoutX="40.0" layoutY="300.0" spacing="40.0" />
      <HBox fx:id="minymax" layoutX="100.0" layoutY="453.0" spacing="70.0" />
      <ImageView fitHeight="22.0" fitWidth="35.0" layoutX="25.0" layoutY="24.0" onMouseClicked="#onClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/backBtn.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
